{
  "name": "graphql-yoga-waterline",
  "version": "0.0.6",
  "description": "an easy implementation of the graphql-yoga server, but with Waterline ORM implemented, for easy Setup of DB or FileStore driven APIs",
  "main": "dist/index.js",
  "bin": {
    "graphql-yoga-waterline": "./dist/lib/cli.js"
  },
  "dependencies": {
    "@okgrow/graphql-scalars": "^0.4.3",
    "async": "^2.6.1",
    "glob": "^7.1.3",
    "graphql": "^14.1.1",
    "graphql-shield": "^5.1.2",
    "graphql-yoga": "^1.17.4",
    "lodash": "^4.17.11",
    "mkdirp": "^0.5.1",
    "sails-disk": "^1.1.0",
    "template-file": "^3.0.1",
    "waterline": "^0.13.6",
    "waterline-utils": "^1.4.2",
    "yargs": "^12.0.5"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.2.5",
    "babel-cli": "^6.26.0",
    "babel-preset-minify": "^0.5.0",
    "babel-preset-node8": "^1.2.0",
    "dotenv": "^6.2.0",
    "nodemon": "^1.18.9",
    "rimraf": "^2.6.3"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npx rimraf dist && npx babel --out-dir dist src && npm run copy:templates",
    "copy:templates": "cp -R ./src/templates ./dist/templates",
    "build:minify": "npx rimraf dist && npx babel --out-dir dist src --no-comments --minify && npm run copy:templates",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joeherold/graphql-yoga-waterline.git"
  },
  "keywords": [
    "graphql",
    "sails",
    "waterline",
    "yoga"
  ],
  "author": "Johannes Pichler <j.pichler@webpixels.at>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joeherold/graphql-yoga-waterline/issues"
  },
  "homepage": "https://github.com/joeherold/graphql-yoga-waterline#readme",
  "babel": {
    "presets": [
      "node8",
      "minify"
    ]
  },
  "engines": {
    "node": ">=8"
  }
}
